				</tween:To>
				<tween:To duration="750">
					<tween:props>
						<e:Object scaleX="{3}" scaleY="{3}" alpha="{0.4}" />
					</tween:props>
				</tween:To>
				<tween:To duration="750">
					<tween:props>
						<e:Object scaleX="{3.5}" scaleY="{3.5}" alpha="{0.2}" />
					</tween:props>
				</tween:To>
				<tween:To duration="750">
					<tween:props>
						<e:Object alpha="{0}" scaleX="{4}" scaleY="{4}" />
					</tween:props>
				</tween:To>
			</tween:TweenItem>
		</tween:TweenGroup>
