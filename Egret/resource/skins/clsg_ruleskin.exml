							<e:Group width="680" x="20" y="118" anchorOffsetY="0">
								<e:Image x="20" y="0" source="icon_paytable" width="60" height="60" />
								<e:Group width="680" x="0" scaleX="1" scaleY="1" top="0">
									<e:Label id="rulelabel55" text="赔付表" y="10" width="530" size="25" scaleX="1" scaleY="1" x="150" textColor="0xCCCCCC"/>
									<e:Label id="rulelabel56" text="·  显示中奖组合以及赔付表。" y="50" width="530" size="24" scaleX="1" scaleY="1" x="150" textColor="0xCCCCCC" lineSpacing="5"/>
									<e:layout>
									<e:VerticalLayout paddingLeft="147" gap="20"/>
									</e:layout>
								</e:Group>


							</e:Group>
							<e:Group width="680" x="20" y="236" anchorOffsetY="0">
								<e:Image x="20" y="0" source="icon_rule_book" width="60" height="60" />
								<e:Group width="680" x="0" scaleX="1" scaleY="1" top="0">
									<e:Label id="rulelabel57" text="规则" y="10" width="530" size="25" scaleX="1" scaleY="1" x="150" textColor="0xCCCCCC"/>
									<e:Label id="rulelabel58" text="·  显示游戏规则和按键功能。" y="50" width="530" size="24" scaleX="1" scaleY="1" x="150" textColor="0xCCCCCC" lineSpacing="5"/>
									<e:layout>
									<e:VerticalLayout paddingLeft="147" gap="20"/>
