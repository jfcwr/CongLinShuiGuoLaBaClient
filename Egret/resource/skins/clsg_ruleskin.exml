									<e:Label id="rulelabel60" text="·  点击回到游戏大厅。" y="50" width="530" size="24" scaleX="1" scaleY="1" x="150" textColor="0xCCCCCC" lineSpacing="5"/>
									<e:layout>
									<e:VerticalLayout paddingLeft="147" gap="20"/>
